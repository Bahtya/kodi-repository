{
    "routes": [
        {
            "src": "/",
            "dest": "index.html"
        },
        {
            "src": "/apitmdb/(.*)",
            "dest": "https://api.themoviedb.org/$1"
        },
        {
            "src": "/repo",
            "dest": "/B"
        },
        {
            "src": "/release/repo",
            "dest": "/release/repository.xbmc.org.zip"
        },
        {
            "src": "/release/tmdb",
            "dest": "/release/chinese.metadata.themoviedb.org.zip"
        },
        {
            "src": "/release/tvdb",
            "dest": "/release/chinese.metadata.tvshows.themoviedb.org.zip"
        }
    ]
}